pipeline {
    agent any
    stages{
    stage('Mavan') {
            when {
               branch 'develop'
            }
            steps {
                 echo "Deploy to Dev"
            }
        }
        stage('Tomcat-Depl-prod') {
            when {
               branch 'main'
            }
            steps {
                 echo "Deploy to prod"
            }
        }
    }  
  }

    
