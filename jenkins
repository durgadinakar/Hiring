pipeline {
    agent any
    stages{
      stage('Mavan') {
            steps {
                 sh "mvn clean package"
            }
        }
        stage('Docker Build') {    
            steps {
                 sh "docker build -t gmtjdk/hiring:0.0.3 ."
            }
        }
        stage('Docker push'){
               steps {
                     sh "docker login -u gmtjdk -p xxxxxxxxx"
                     sh "docker push gmtjdk/hiring:0.0.3"
               }
        }
    }  
  }

    
